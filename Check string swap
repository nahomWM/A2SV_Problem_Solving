class Solution(object):
    def areAlmostEqual(self, s1, s2):
        sol = 0
        diff = []
        for i in range(len(s1)):
            if s1[i] != s2[i]:
                sol += 1
                diff.append(i)   
        if sol == 0:  
            return True
        if sol != 2:  
            return False
        i, j = diff
        return s1[i] == s2[j] and s1[j] == s2[i]
